// Functions 
fun int AddOne(int x)
{
    // one input locally know as x
    // returns an Integer
    
    int result; // variable to store final answer
    x+1 => result; // add one to input x
    return result; // output final answer
    
}

// Main Program
AddOne(8) => int Answer; // Call AddOne Function
<<<Answer>>>; // Print out Result of AddOne()

